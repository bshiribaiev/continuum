{
  "manifest_version": 3,
  "name": "Continuum",
  "version": "0.1.0",
  "permissions": ["storage"],
  "host_permissions": ["http://localhost:8080/*"],
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}